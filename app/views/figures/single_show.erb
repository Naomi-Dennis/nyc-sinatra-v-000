<h2> Figure View </h2>
<h3>Name: <%= @figure.name %> </h3>

<h4> Landmarks </h4>

<ul>
<% @landmarks.each do | landmark | %>
  <li> <%= landmark.name %> </li>
  <%  end %>
</ul>

<h4> Titles </h4>
<ul>
<%  @titles.each do | title | %>
  <li><%= title.name %></li>
<% end %>
</ul>

<a href="/figures/<%= @figure.id %>/edit">Edit </a>
<a href="/figures">View All</a>
