<h3> Figures </h3>
<% @figures.each do | figure | %>
  Name: <%= figure.name %>
  <h4> Landmarks </h4>
  <ul>
  <% @landmarks.each do | landmark | %>
    <li> <%= landmark.name %> </li>
    <%  end %>
</ul>
  <h4> Titles </h4>
  <ul>
<%  @titles.each do | title | %>
    <li><%= title.name %></li>
<% end %>
</ul>

  <a href="/figures/<%= figure.id %>">View</a>
  &nbsp; &nbsp;
  <a href="/figures/<%= figure.id %>/edit">Edit</a>
  <br/><br/>
<% end %>

<br/><br/>
